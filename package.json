{
  "name": "@tiangong-ai/vis-server",
  "version": "0.0.4",
  "description": "A high-performance chart rendering service based on @antv/gpt-vis-ssr with cluster support, concurrency control, and HTTP API",
  "keywords": [
    "visualization",
    "chart",
    "render",
    "server",
    "antv",
    "gpt-vis",
    "ssr",
    "tiangong",
    "cluster",
    "express"
  ],
  "license": "MIT",
  "author": "Nan LI",
  "type": "module",
  "bin": {
    "tiangong-ai-vis-private": "dist/src/cli.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TianGong-AI/tiangong-ai-vis-private"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc && shx chmod +x dist/src/*.js",
    "build:clean": "shx rm -rf dist",
    "start": "npm run build && node dist/src/index.js",
    "start:env": "npm run build && npx dotenv -e .env -- node dist/src/index.js",
    "test": "node test/tidas_validation_test.js",
    "lint": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "ncu": "npx npm-check-updates",
    "ncu:update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@antv/gpt-vis-ssr": "^0.3.1",
    "@types/express": "^5.0.3",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "dotenv-cli": "^10.0.0",
    "npm-check-updates": "^19.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "shx": "^0.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
